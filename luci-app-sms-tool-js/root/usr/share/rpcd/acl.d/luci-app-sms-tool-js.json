{
	"luci-app-sms-tool-js": {
		"description": "Grant access to LuCI app sms-tool-js",
		"read": {
			"cgi-io": [ "exec", "upload" ],
    			"file": {
    				"/sys/class/leds": [ "list" ],
    				"/dev": [ "list" ],
				"/usr/bin/sms_tool": [ "exec" ],
				"/etc/init.d/my_new_sms start" : [ "exec" ],
				"/etc/init.d/my_new_sms stop" : [ "exec" ],
				"/etc/init.d/my_new_sms enable" : [ "exec" ],
				"/etc/init.d/my_new_sms disable" : [ "exec" ],
				"/etc/init.d/my_new_sms restart" : [ "exec" ],
				"/etc/init.d/led restart" : [ "exec" ],
				"/sbin/smstool_led.sh": [ "exec" ],
				"/sbin/sms_led.sh": [ "exec" ],
				"/bin/sh": [ "exec" ],
				"/bin/tar": [ "exec" ],
				"/bin/mkdir": [ "exec" ],
				"/etc/crontabs/root": [ "read" ],
				"/usr/bin/opkg list-installed": ["exec"],
				"/usr/libexec/package-manager-call list-installed": [ "exec" ],
				"/usr/libexec/opkg-list installed": [ "exec" ],
				"/usr/libexec/opkg-call list-installed": [ "exec" ],
				"/etc/sms_tool_js": [ "read" ],
				"/etc/defmodems": [ "read" ],
				"/tmp/mysms.txt": [ "read" ],
				"/tmp/atcmmds_backup.tar.gz": [ "read" ],
				"/tmp/atcmmds_upload.tar.gz": [ "read" ],
				"/tmp/ussdcodes_backup.tar.gz": [ "read" ],
				"/tmp/ussdcodes_upload.tar.gz": [ "read" ],
				"/etc/modem/atcmmds.user": [ "read" ],
				"/etc/modem/atcmmds": [ "list" ],
				"/etc/modem/atcmmds/*": [ "read" ],
				"/etc/modem/atcmmds/*.user": [ "read" ],
				"/etc/modem/phonebook.user": [ "read" ],
				"/etc/modem/ussdcodes": [ "list" ],
				"/etc/modem/ussdcodes/*": [ "read" ],
				"/etc/modem/ussdcodes/*.user": [ "read" ],
				"/etc/modem/ussdcodes.user": [ "read" ]
			},
			"uci": [ "sms_tool_js", "luci-app-sms-tool-js", "defmodems" ],
			"ubus": {
				"file": [ "read" ],
				"sms_forward": [ "forward" ]
			}
		},
		"write": {
			"cgi-io": [ "upload" ],
			"file": {
				"/etc/sms_tool_js": [ "write" ],
				"/etc/crontabs/root": [ "write" ],
				"/etc/init.d/cron restart": [ "exec" ],
				"/bin/chmod": [ "exec" ],
				"/bin/rm": [ "exec" ],
				"/bin/tar": [ "exec" ],
				"/bin/mkdir": [ "exec" ],
				"/sys/class/leds/*/brightness": [ "write" ],
				"/tmp/mysms.txt": [ "write" ],
				"/tmp/atcmmds_backup.tar.gz": [ "write" ],
				"/tmp/atcmmds_upload.tar.gz": [ "write" ],
				"/tmp/ussdcodes_backup.tar.gz": [ "write" ],
				"/tmp/ussdcodes_upload.tar.gz": [ "write" ],
				"/etc/modem/atcmmds.user": [ "write" ],
				"/etc/modem/atcmmds/*.user": [ "write" ],
				"/etc/modem/atcmmds/*": [ "write" ],
				"/etc/modem/phonebook.user": [ "write" ],
				"/etc/modem/ussdcodes/*.user": [ "write" ],
				"/etc/modem/ussdcodes/*": [ "write" ],
				"/etc/modem/ussdcodes.user": [ "write" ]
			},
			"ubus": {
				"file": [ "read", "write", "remove" ]
			},
			"uci": [ "sms_tool_js", "luci-app-sms-tool-js" ]
		}
	}
}
